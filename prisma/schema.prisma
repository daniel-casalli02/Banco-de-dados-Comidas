generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model fastfood{
  id            Int @id @default(autoincrement())
  nome          String
  tipo          String
  preco         Float
  descricao     String

  @@map ("fastfood")
}
